{
  "name": "ai-timetabler",
  "version": "1.0.0",
  "description": "AI-powered timetabling system for academic institutions",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "migrate": "ts-node src/scripts/migrate.ts up",
    "migrate:status": "ts-node src/scripts/migrate.ts status",
    "migrate:reset": "ts-node src/scripts/migrate.ts reset",
    "db:setup": "npm run migrate",
    "db:reset": "npm run migrate:reset && npm run migrate",
    "db:seed:test": "ts-node src/scripts/seedTestData.ts"
  },
  "keywords": [
    "timetabling",
    "scheduling",
    "ai",
    "optimization"
  ],
  "author": "AI Timetabler Team",
  "license": "MIT",
  "dependencies": {
    "@types/axios": "^0.9.36",
    "@types/multer": "^2.0.0",
    "@types/pdfkit": "^0.17.3",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "chardet": "^2.1.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fuse.js": "^7.1.0",
    "helmet": "^7.1.0",
    "iconv-lite": "^0.7.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.55.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/chardet": "^0.8.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "playwright": "^1.55.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
